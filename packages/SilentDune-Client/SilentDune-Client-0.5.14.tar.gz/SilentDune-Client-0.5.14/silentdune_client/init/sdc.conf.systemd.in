[settings]
; Silent Dune Client Configuration File
; This file was automatically generated by the installer.
; Run "sdc-install --help" to configure this file.

logfile=/var/log/silentdune.log

[firewall]
; Silent Dune Firewall Configuration

; Set allowed networks this system can connect to.
; List of networks in CIDR notation, delimited with spaces.
; Empty value allows access to all networks.
allowed_networks=

; Disable the auto SSH rule generation.
disable_auto_ssh=no
