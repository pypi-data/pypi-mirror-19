<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="collective.tiles.unitegallery">

  <records prefix="plone.app.mosaic.app_tiles.unite_gallery"
     interface="plone.app.mosaic.interfaces.ITile" i18n:domain="collective.tiles.unitegallery">
    <value key="name">plone.tiles.unite_gallery</value>
    <value key="label" i18n:translate="">Unite Gallery</value>
    <value key="category">advanced</value>
    <value key="tile_type">app</value>
    <value key="default_value"></value>
    <value key="read_only">false</value>
    <value key="settings">true</value>
    <value key="favorite">false</value>
    <value key="rich_text">false</value>
    <value key="weight">20</value>
  </records>

  <records prefix="plone.resources/tiles-unitegallery"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'>
    <value key="url">++resource++collective.tiles.unitegallery</value>
    <value key="js">++resource++collective.tiles.unitegallery/js/unitegallery.min.js</value>
    <value key="css">
      <element>++resource++collective.tiles.unitegallery/css/unite-gallery.css</element>
    </value>
  </records>

</registry>
