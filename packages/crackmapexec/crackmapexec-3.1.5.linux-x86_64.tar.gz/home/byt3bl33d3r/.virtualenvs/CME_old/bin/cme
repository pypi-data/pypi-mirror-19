#!/home/byt3bl33d3r/.virtualenvs/CME_old/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'crackmapexec==3.1.5','console_scripts','cme'
__requires__ = 'crackmapexec==3.1.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('crackmapexec==3.1.5', 'console_scripts', 'cme')()
    )
