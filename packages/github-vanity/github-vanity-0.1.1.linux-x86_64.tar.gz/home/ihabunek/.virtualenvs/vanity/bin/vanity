#!/home/ihabunek/.virtualenvs/vanity/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'github-vanity==0.1.1','console_scripts','vanity'
__requires__ = 'github-vanity==0.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('github-vanity==0.1.1', 'console_scripts', 'vanity')()
    )
