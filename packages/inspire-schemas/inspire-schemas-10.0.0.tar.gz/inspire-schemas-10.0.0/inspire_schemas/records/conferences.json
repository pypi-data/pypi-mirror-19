{
    "$schema": "http://json-schema.org/schema#",
    "properties": {
        "acronym": {
            "items": {
                "type": "string"
            },
            "title": "Conference acronym",
            "type": "array"
        },
        "address": {
            "items": {
                "$ref": "elements/address.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "alternative_titles": {
            "items": {
                "$ref": "elements/title.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "closing_date": {
            "format": "date",
            "title": "Conference closing date",
            "type": "string"
        },
        "cnum": {
            "$ref": "elements/cnum.json"
        },
        "contact_details": {
            "items": {
                "$ref": "elements/contact.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "deleted": {
            "type": "boolean"
        },
        "inspire_categories": {
            "items": {
                "$ref": "elements/inspire_field.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "keywords": {
            "items": {
                "properties": {
                    "source": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "title": "Keywords",
            "type": "array",
            "uniqueItems": true
        },
        "new_record": {
            "$ref": "elements/json_reference.json",
            "description": "Master record that replaces this record",
            "title": "New record"
        },
        "nonpublic_note": {
            "title": "Non public note",
            "type": "string"
        },
        "note": {
            "items": {
                "type": "string"
            },
            "title": "Note",
            "type": "array"
        },
        "opening_date": {
            "format": "date",
            "title": "Conference opening date",
            "type": "string"
        },
        "place": {
            "pattern": "^.+,.+,?.*$",
            "title": "Conference place",
            "type": "string"
        },
        "self": {
            "$ref": "elements/json_reference.json",
            "description": "Url of the record itself",
            "title": "Url of the record"
        },
        "series": {
            "items": {
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "number": {
                        "type": "integer"
                    }
                },
                "required": [
                    "name"
                ],
                "type": "object"
            },
            "title": "Conference series to which a conference belongs.",
            "type": "array",
            "uniqueItems": true
        },
        "short_description": {
            "items": {
                "properties": {
                    "source": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "title": "Short description",
            "type": "array"
        },
        "titles": {
            "items": {
                "$ref": "elements/title.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "urls": {
            "items": {
                "$ref": "elements/url.json"
            },
            "type": "array",
            "uniqueItems": true
        }
    },
    "title": "Conference",
    "type": "object"
}
