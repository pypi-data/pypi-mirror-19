<?xml version="1.0"?>
<object name="portal_actions" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
   <object name="user" meta_type="CMF Action Category">
     <object name="pmr2_oauth_apps" meta_type="CMF Action" i18n:domain="pmr2.oauth" insert-after="preferences">
      <property name="title" i18n:translate="">Apps Manager</property>
      <property name="description" i18n:translate=""></property>
      <property name="url_expr">string:${portal_url}/issued_oauth_tokens</property>
      <property name="icon_expr"></property>
      <property name="available_expr">python:member is not None</property>
      <property name="permissions">
        <element value="Set own properties"/>
      </property>
      <property name="visible">True</property>
     </object>
  </object>
</object>
