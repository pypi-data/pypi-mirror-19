SET DOCUMENT Name = "Worst. BEL Document. Ever."
SET DOCUMENT Description = "This document outlines all of the evil and awful work that is possible during BEL curation"
SET DOCUMENT Version = "0.0"
SET DOCUMENT Authors = "Charles Tapley Hoyt"
SET DOCUMENT Licenses = "WTF License"
SET DOCUMENT ContactInfo = "charles.hoyt@scai.fraunhofer.de"

DEFINE NAMESPACE CHEBI AS URL "http://resources.openbel.org/belframework/20150611/namespace/chebi.belns"
DEFINE NAMESPACE HGNC AS URL "http://resources.openbel.org/belframework/20150611/namespace/hgnc-human-genes.belns"
DEFINE NAMESPACE MESHD AS URL "http://resources.openbel.org/belframework/20150611/namespace/mesh-diseases.belns"
DEFINE NAMESPACE GO AS URL "http://resources.openbel.org/belframework/20150611/go-biological-process.belns"

DEFINE ANNOTATION CellLine AS  URL "http://resources.openbel.org/belframework/20150611/annotation/cell-line.belanno"
DEFINE ANNOTATION TextLocation AS  LIST {"Abstract","Results","Legend","Review"}
DEFINE ANNOTATION Disease AS  URL "http://resources.openbel.org/belframework/20150611/annotation/mesh-diseases.belanno"

# Name doesn't match annotation file Keyword
DEFINE ANNOTATION Specieses AS  URL "http://resources.openbel.org/belframework/20150611/annotation/species-taxonomy-id.belanno"


SET STATEMENT_GROUP = "Group 1"

# Forgot quotes
SET Disease = Atherosclerosis

# Typo
SET Disease = "Artereis"
SET TextLocation = "Review"

# 
SET Evidence = "Oxidation and nitration of macromolecules, such as proteins, DNA and lipids, are prominent
in atherosclerotic arteries."

# Missing citation information
SET Citation = {"PubMed","Trends in molecular medicine","12928037","","de Nigris F|Lerman A|Ignarro LJ|Williams-Ignarro S|Sica V|Baker AH|Lerman LO|Geng YJ|Napoli C",""}

# Missing newline escapes
SET Evidence = "Arterial cells are highly susceptible to oxidative stress, which can induce both necrosis
and apoptosis (programmed cell death) [1,2]"

biologicalProcess(GO:"response to oxidative stress") increases biologicalProcess(GO:"apoptotic process")

# Naked name
biologicalProcess("response to oxidative stress") increases biologicalProcess(GO:necrosis)

# Missing name
biologicalProcess(GO:"maybe response to oxidative stress") increases biologicalProcess(GO:necrosis)

# Mixed up arguments
proteinAbundance(HGNC:TP53,substitution(Q,248,R)) directlyDecreases transcriptionalActivity(proteinAbundance(HGNC:TP53))
