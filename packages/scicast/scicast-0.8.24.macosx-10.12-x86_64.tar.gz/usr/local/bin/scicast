#!/usr/local/opt/python3/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'scicast==0.8.24','console_scripts','scicast'
__requires__ = 'scicast==0.8.24'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('scicast==0.8.24', 'console_scripts', 'scicast')()
    )
